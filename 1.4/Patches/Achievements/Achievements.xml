<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/MainButtonDef[defName="Achievements"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>Defs</xpath>
          <value>
            <AchievementsExpanded.AchievementTabDef>
              <defName>Mlie_Originals</defName>
              <label>Mlies Mods</label>
            </AchievementsExpanded.AchievementTabDef>
          </value>
        </li>
        <li Class="PatchOperationConditional">
          <success>Always</success>
          <xpath>/Defs/AchievementsExpanded.AchievementDef[@Name="Mlie_OriginalAchievementParent"]</xpath>
          <nomatch Class="PatchOperationAdd">
            <xpath>Defs</xpath>
            <value>
              <AchievementsExpanded.AchievementDef Name="Mlie_OriginalAchievementParent" Abstract="true">
                <bgtexPath>Achievements/OriginalAchievmentsBG</bgtexPath>
                <tab>Mlie_Originals</tab>
              </AchievementsExpanded.AchievementDef>
            </value>
          </nomatch>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs</xpath>
          <value>
            <AchievementsExpanded.AchievementDef ParentName="Mlie_OriginalAchievementParent">
              <defName>CAN_NothingWasted</defName>
              <label>Nothing wasted</label>
              <description>Obtain 20 simple insect meals</description>
              <tracker Class="AchievementsExpanded.ItemTracker">
                <def>CAN_InsectMealSimple</def>
                <count>20</count>
              </tracker>
              <texPath>CAN_InsectSimple</texPath>
              <points>10</points>
            </AchievementsExpanded.AchievementDef>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs</xpath>
          <value>
            <AchievementsExpanded.AchievementDef ParentName="Mlie_OriginalAchievementParent">
              <defName>CAN_TryToIgnoreTheTaste</defName>
              <label>Try to ignore the taste</label>
              <description>Obtain 20 fine insect meals</description>
              <tracker Class="AchievementsExpanded.ItemTracker">
                <def>CAN_InsectMealFine</def>
                <count>20</count>
              </tracker>
              <texPath>CAN_InsectFine</texPath>
              <points>15</points>
            </AchievementsExpanded.AchievementDef>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs</xpath>
          <value>
            <AchievementsExpanded.AchievementDef ParentName="Mlie_OriginalAchievementParent">
              <defName>CAN_ActuallyNotThatBad</defName>
              <label>Actually not that bad</label>
              <description>Obtain 20 lavish insect meals</description>
              <tracker Class="AchievementsExpanded.ItemTracker">
                <def>CAN_InsectMealLavish</def>
                <count>20</count>
              </tracker>
              <texPath>CAN_InsectLavish</texPath>
              <points>25</points>
            </AchievementsExpanded.AchievementDef>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>